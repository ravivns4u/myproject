.merchantfig
{
	position: relative;
	width: 100%;
	color: #ffffff;
	text-align: left;
	background-color: #000000;
	font-size: 16px;
	cursor: pointer;margin:0;    margin-top: 45px;
}
.merchantfig * {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-transition: all 0.4s ease-in;
  transition: all 0.4s ease-in;
}
.merchantfig img {
  position: relative;
  max-width: 100%;
  vertical-align: top;
      width: 100%;
}
.merchantfig figcaption {
  position: absolute;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  opacity: 0;
  padding: 20px 20px;
  display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
}
.merchantfig figcaption:before,
.merchantfig figcaption:after {
  width: 1px;
  height: 0;
}
.merchantfig figcaption:before {
  right: 0;
  top: 0;
}
.merchantfig figcaption:after {
  left: 0;
  bottom: 0;
}
.merchantfig h3,
.merchantfig p {
  line-height: 1.5em;
}
.merchantfig h3 {
  margin: 0 0 0 0;
  text-transform: unset;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    justify-content: center;
    display: flex;
    align-items: center;
    font-weight: 700;
    font-size: 25px;
    line-height: 32px;
    text-align: center;
    letter-spacing: 0.01em;
    color: #FFFFFF;
    padding: 0px 20px;
}
.merchantfig p {
  font-weight: normal;
font-size: 16px;
line-height: 24px;
  margin: 0 0 0;
  letter-spacing: 0.01em;

color: #FFFFFF;
}
.merchantfig p span{color:#f1f11c;font-style:italic;font-weight:bold;}
.merchantfig:before,
.merchantfig:after,
.merchantfig figcaption:before,
.merchantfig figcaption:after {
  position: absolute;
  content: '';
  background-color: #ffffff;
  z-index: 1;
  -webkit-transition: all 0.4s ease-in;
  transition: all 0.4s ease-in;
  opacity: 0.8;
}
.merchantfig:before,
.merchantfig:after {
  height: 1px;
  width: 0%;
}
.merchantfig:before {
  top: 0;
  left: 0;
}
.merchantfig:after {
  bottom: 0;
  right: 0;
}
.merchantfig:hover img,
.merchantfig.hover img {
  opacity: 0.4;
}
.merchantfig:hover figcaption,
.merchantfig.hover figcaption {
  opacity: 1;
}
.merchantfig:hover h3
{
opacity: 0;
}
.merchantfig:hover figcaption:before,
.merchantfig.hover figcaption:before,
.merchantfig:hover figcaption:after,
.merchantfig.hover figcaption:after {
  height: 100%;
}
.merchantfig:hover:before,
.merchantfig.hover:before,
.merchantfig:hover:after,
.merchantfig.hover:after {
  width: 100%;
}
.merchantfig:hover:before,
.merchantfig.hover:before,
.merchantfig:hover:after,
.merchantfig.hover:after,
.merchantfig:hover figcaption:before,
.merchantfig.hover figcaption:before,
.merchantfig:hover figcaption:after,
.merchantfig.hover figcaption:after {
  opacity: 0.1;
}

