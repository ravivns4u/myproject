	letter-spacing: 0.01em;
	text-align:center;
	color: #000000;
	margin: 0;
}
.stepscontent .stepsitem:after {
    position: absolute;
    content: '';
    height: auto;
    background: transparent;
    margin: 0;
    transform: translateY(-50%) translateX(-1px);
    width: 100%;
    left: 50%;
    top: 60px;
    border-bottom: 3px dotted #262423;
}
.stepscontent .stepsitem:last-child {
    margin-right: 0 !important;
}
.stepscontent .stepsitem:last-child:after{display:none}
.stepsitem:hover .steptext {
    background: #fff !important;
    color: #fff;
}