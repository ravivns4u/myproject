@import '../../styles/root.scss';

.AboutPageSection
{
	margin-top:87px;
}

.TopBannerSection
{
	background-color: rgba(0, 0, 0, 0.5);
    background-blend-mode: soft-light;
	background-size: cover;
}
.TopBannerDiv
{
	padding: 10% 15%;
    text-align: center;
}
.TopBannerHeading
{
	font-weight: 700;
    font-size: 55px;
    line-height: 65px;
    letter-spacing: 0.01em;
    color: #FFFFFF;
    margin-bottom: 15px;
}
.TopBannerPara
{
	font-weight: 500;
	font-size: 25px;
	line-height: 34px;
	letter-spacing: 0.01em;
	color: #FFFFFF;
	text-shadow: 2px 1px 1px rgba(0, 0, 0, 0.9);
}
.OurServicesSection
{
	padding: 2% 10%;
	float: left;
    width: 100%;
}
.OurServicesSection h2
{
	font-weight: 600;
    font-size: 40px;
    line-height: 40px;
    text-align: center;
    letter-spacing: 0.01em;
    color: #000000;
    text-transform: uppercase;
    margin: 0px 0px 10px 0px;
}
.OurServicesSection h3
{
	font-weight: 500;
	font-size: 30px;
    line-height: 30px;
    text-align: center;
    letter-spacing: 0.01em;
    color: #000000;
	margin: 0px 0px 10px 0px;
}

.timeline
{
	display: flex;
	flex-direction: column;
	margin: 20px auto 40px auto;
	position: relative;
}
.timeline__event
{
	margin-bottom: 20px;
	position: relative;
	display: flex;
	margin: 20px 0;
	border-radius: 6px;
	align-self: center;
	width: 60vw;
}
.timeline__event:nth-child(2n+1)
{
	flex-direction: row-reverse;
}
.timeline__event:nth-child(2n+1) .timeline__event__date
{
	border-radius: 0 6px 6px 0;
}
.timeline__event:nth-child(2n+1) .timeline__event__content
{
	border-radius: 6px 0 0 6px;
}
.timeline__event:nth-child(2n+1) .timeline__event__icon:before
{
	content: "";
	width: 2px;
	height: 100%;
	background: #414141;
	position: absolute;
	top: 0%;
	left: 50%;
	right: auto;
	z-index: -1;
	transform: translateX(-50%);
	-webkit-animation: fillTop 2s forwards 4s ease-in-out;
    animation: fillTop 2s forwards 4s ease-in-out;
}
.timeline__event:nth-child(2n+1) .timeline__event__icon:after
{
	content: "";
	width: 100%;
	height: 2px;
	background: #414141;
	position: absolute;
	right: 0;
	z-index: -1;
	top: 50%;
	left: auto;
	transform: translateY(-50%);
	-webkit-animation: fillLeft 2s forwards 4s ease-in-out;
    animation: fillLeft 2s forwards 4s ease-in-out;
}
.timeline__event__title
{
	font-size: 18px;
    line-height: 1.4;
    text-transform: uppercase;
    font-weight: bold;
    color: #f1f11c;
    letter-spacing: 1.5px;
    font-style: italic;
}
.timeline__event__content
{
	padding: 20px;
	box-shadow: 0 30px 60px -12px rgba(50, 50, 93, 0.25), 0 18px 36px -18px rgba(0, 0, 0, 0.3), 0 -12px 36px -8px rgba(0, 0, 0, 0.025);
	background: #414141;
	width: calc(40vw - 84px);
	border-radius: 0 6px 6px 0;
}
.timeline__event__date
{
	color: #000;
	text-transform:uppercase;
	font-size: 1.2rem;
	font-weight: 600;
	background: #FFFF00;
	display: flex;
	align-items: center;
	justify-content: center;
	white-space: unset;text-align: center;
	padding: 0 20px;
	border-radius: 6px 0 0 6px;width: 15vw;
}
.timeline__event__icon
{
	display: flex;
	align-items: center;
	justify-content: center;
	color: #9251ac;
	padding: 20px;
	align-self: center;
	margin: 0 20px;
	background: #000;
	border-radius: 100%;
	width: 50px;
	box-shadow: 0 30px 60px -12px rgba(50, 50, 93, 0.25), 0 18px 36px -18px rgba(0, 0, 0, 0.3), 0 -12px 36px -8px rgba(0, 0, 0, 0.025);
	padding: 50px;
	height: 50px;
	position: relative;
}
.timeline__event__icon img
{
max-width:50px;
}
.timeline__event__icon:before
{
	content: "";
	width: 2px;
	height: 100%;
	background: #414141;
	position: absolute;
	top: 0%;
	z-index: -1;
	left: 50%;
	transform: translateX(-50%);
	-webkit-animation: fillTop 2s forwards 4s ease-in-out;
    animation: fillTop 2s forwards 4s ease-in-out;
}
.timeline__event__icon:after
{
	content: "";
	width: 100%;
	height: 2px;
	background: #414141;
	position: absolute;
	left: 0%;
	z-index: -1;
	top: 50%;
	transform: translateY(-50%);
	-webkit-animation: fillLeftOdd 2s forwards 4s ease-in-out;
    animation: fillLeftOdd 2s forwards 4s ease-in-out;
}
.timeline__event__description
{
	flex-basis: 60%;
	font-weight: normal;
    font-size: 17px;
    line-height: 25px;
    letter-spacing: 0.01em;
    color: #fff;
}
.timeline__eventtype2:after
{
	background: #555ac0;
}


.timeline__event:last-child .timeline__event__icon:before
{
	content: none;
}

@media (max-width: 786px)
{
	.timeline__event 
	{
		flex-direction: column;
		align-self: center;
	}
	.timeline__event__content
	{
		width: 100%;
	}
	.timeline__event__icon
	{
		border-radius: 6px 6px 0 0;
		width: 100%;
		margin: 0;
		box-shadow: none;
	}
	.timeline__event__icon:before, .timeline__event__icon:after
	{
		display: none;
	}
	.timeline__event__date
	{
		border-radius: 0;
		padding: 20px;
	}
	.timeline__event:nth-child(2n+1)
	{
		flex-direction: column;
		align-self: center;
	}
	.timeline__event:nth-child(2n+1) .timeline__event__date
	{
		border-radius: 0;
		padding: 20px;
	}
	.timeline__event:nth-child(2n+1) .timeline__event__icon
	{
		border-radius: 6px 6px 0 0;
		margin: 0;
	}
}
@-webkit-keyframes fillLeft
{
  100% {
    right: 100%;
  }
}
@keyframes fillLeft {
  100% {
    right: 100%;
  }
}
@-webkit-keyframes fillTop {
  100% {
    top: 100%;
  }
}
@keyframes fillTop {
  100% {
    top: 100%;
  }
}
@-webkit-keyframes fillLeftOdd {
  100% {
    left: 100%;
  }
}
@keyframes fillLeftOdd {
  100% {
    left: 100%;
  }
}

