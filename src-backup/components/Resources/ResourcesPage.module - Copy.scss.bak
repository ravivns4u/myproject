    margin-bottom: 15px;
    font-size: 20px;

  }

  .copyrightSection  {
    margin-top: 15px; 
    flex-wrap: wrap;
    justify-content: left;
  }

  .copyrightSection div {
    margin-bottom: 5px;
    width: 100%;
  }

 .footerGrid1, .footerGrid2, .footerGrid3{
   padding-top: 0px;

  }

}
