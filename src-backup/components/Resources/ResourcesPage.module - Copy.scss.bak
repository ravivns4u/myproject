@media only screen and (max-width:949px)
{
  .gridDivision{
    grid-template-rows: repeat(3, 1fr);
    grid-template-columns: 1fr;
  }
  .footerGrid1, .footerGrid2, .footerGrid3{
    padding: 1em 0;
  }
  .thirdPart{
    display: flex;
    align-items: center;
    margin: 2rem 0;
    width: 100%;
    grid-row-gap: 2rem;
    row-gap: inherit;
    grid-column-gap: 2rem;
    -moz-column-gap: 2rem;
    column-gap: 2rem;

  }
  .feedImg {
    width:42%;
    margin: inherit;
  }
  .itsTime, .aGreat, .breakFree{
    text-align: center;
  }
  .midBtn{
    margin:0 auto;
    width:100%;   
  }
}
@media only screen and (max-width:610px){
.copyrightSection{
  flex-direction: column;
}

}
.footerGrid3.HdeDestop {
  display: none;
}
@media screen and (max-width: 991px) {
.thirdPart br {
    display: none;
  }
  .gridDivision {
    display: block;
  } 
}

@media screen and (max-width: 767px) {
